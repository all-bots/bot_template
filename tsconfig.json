{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": false
  },
  "exclude": ["node_modules"],
  "include": ["global.d.ts", "thirdparty.d.ts", "src", "public"]
}
